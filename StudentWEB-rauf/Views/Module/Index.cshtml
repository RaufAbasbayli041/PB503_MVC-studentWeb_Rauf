@model List<CourseModuleDTO>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Description</th>
            <th scope="col">title</th>
      
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var module in Model)
            {
                <tr>
                    <th scope="row">@module.Id</th>
                    <td>@module.Description</td>
                    <td>@module.Title</td>
                </tr>
            }
        }

    </tbody>
</table>